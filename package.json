{
  "name": "tilos-client",
  "description": "new client to tilos website",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": "12.18.0"
  },
  "scripts": {
    "dev": "sapper dev",
    "build": "npx sapper build",
    "export": "sapper export",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "lint": "eslint src/** --ignore-path .gitignore",
    "prettier": "prettier --write 'src/**' 'cypress/**'"
  },
  "dependencies": {
    "compression": "1.7.4",
    "date-fns": "2.14.0",
    "encoding": "0.1.12",
    "howler": "2.2.0",
    "polka": "0.5.2",
    "sirv": "1.0.0",
    "timezone-support": "2.0.2"
  },
  "devDependencies": {
    "cypress": "4.8.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-svelte3": "2.7.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.9",
    "npm-run-all": "4.1.5",
    "sapper": "0.27.15",
    "svelte": "3.23.2",
    "prettier": "2.0.5",
    "prettier-plugin-svelte": "1.1.0",
    "svelte-loader": "2.13.6",
    "webpack": "4.43.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier"
  }
}
